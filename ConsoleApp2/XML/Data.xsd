<xs:schema xmlns="http://creditinfo.com/schemas/Sample/Data" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://creditinfo.com/schemas/Sample/Data" elementFormDefault="qualified">
  <xs:element name="Batch">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Contract" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ContractCode">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:pattern value="(\S.{0,30}\S)|(\S)"/>
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="ContractData">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element type="Contract.PhaseOfContract" name="PhaseOfContract"/>
                    <xs:element name="OriginalAmount" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Value">
                            <xs:simpleType>
                              <xs:restriction base="xs:decimal">
                                <xs:pattern value="\d{1,16}(\.\d{1,2})?"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element type="Common.Currency" name="Currency"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="InstallmentAmount" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Value">
                            <xs:simpleType>
                              <xs:restriction base="xs:decimal">
                                <xs:pattern value="\d{1,16}(\.\d{1,2})?"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element type="Common.Currency" name="Currency"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="CurrentBalance">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Value">
                            <xs:simpleType>
                              <xs:restriction base="xs:decimal">
                                <xs:pattern value="\d{1,16}(\.\d{1,2})?"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element type="Common.Currency" name="Currency"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="OverdueBalance" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Value">
                            <xs:simpleType>
                              <xs:restriction base="xs:decimal">
                                <xs:pattern value="\d{1,16}(\.\d{1,2})?"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element type="Common.Currency" name="Currency"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="DateOfLastPayment" minOccurs="0" type="xs:date"/>
                    <xs:element name="NextPaymentDate" minOccurs="0" type="xs:date"/>
                    <xs:element name="DateAccountOpened" minOccurs="0" type="xs:date"/>
                    <xs:element name="RealEndDate" minOccurs="0" type="xs:date"/>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Individual" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CustomerCode">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:pattern value="\S.{0,30}\S"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="FirstName">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:pattern value="\S.{0,62}\S|\S"/>
                          <xs:maxLength value="64"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="LastName">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:pattern value="\S.{0,126}\S"/>
                          <xs:maxLength value="128"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element type="Gender.NotSpecified" name="Gender" minOccurs="0"/>
                    <xs:element name="DateOfBirth" minOccurs="0" type="xs:date"/>
                    <xs:element name="IdentificationNumbers">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NationalID" minOccurs="0">
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:pattern value="[A-Z]{2}\d{7}"/>
                                <xs:maxLength value="32"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="SubjectRole" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="CustomerCode">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:pattern value="\S.{0,30}\S"/>
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element type="Common.RoleOfCustomer" name="RoleOfCustomer"/>
                    <xs:element name="GuaranteeAmount" minOccurs="0">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Value">
                            <xs:simpleType>
                              <xs:restriction base="xs:decimal">
                                <xs:pattern value="\d{1,16}(\.\d{1,2})?"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element type="Common.Currency" name="Currency"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
